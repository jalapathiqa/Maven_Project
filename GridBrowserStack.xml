<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="BrowserStack" thread-count="3" parallel="tests" verbose="3">

	<test name="BrowserStack - chrome">
		<parameter name="browser" value="Chrome" />
		<parameter name="browser_version" value="85.0" />
		<parameter name="os" value="OS X" />
		<parameter name="os_version" value="Catalina" />

		<classes>
			<class name="testsOnCloud.testGridOnBrowserStack" />
		</classes>
	</test>

	<test name="BrowserStack - FF">
		<parameter name="browser" value="Firefox" />
		<parameter name="browser_version" value="80.0" />
		<parameter name="os" value="Windows" />
		<parameter name="os_version" value="10" />


		<classes>
			<class name="testsOnCloud.testGridOnBrowserStack" />
		</classes>
	</test>

</suite> 